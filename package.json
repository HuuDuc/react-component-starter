    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "parcel build ",
    "clean": "rm -r dist/",
    "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
    "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
    "test": "jest --config jestconfig.json",
    "watch": "parcel demo/index.html"
  },
  "keywords": [
    "react",
    "typescript",
    "parcel"
  ],
